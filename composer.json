{
    "name": "openeuropa/gisco-geocoding-provider",
    "type": "library",
    "description": "GISCO Geocoding Provider for Geocoder PHP.",
    "keywords": ["gisco", "geocoding"],
    "homepage": "https://github.com/openeuropa/gisco-geocoding-provider",
    "license": "EUPL-1.2",
    "require": {
        "php": ">=8.0",
        "geocoder-php/common-http": "^4.3"
    },
    "require-dev": {
        "geocoder-php/provider-integration-tests": "^1.5",
        "php-http/curl-client": "^2.2",
        "squizlabs/php_codesniffer": "^3.7",
        "phpunit/phpunit": "^9"
    },
    "provide": {
        "geocoder-php/provider-implementation": "1.0"
    },
    "autoload": {
        "psr-4": {
            "OpenEuropa\\Provider\\GiscoGeocoding\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "OpenEuropa\\Provider\\GiscoGeocoding\\Tests\\": "test/"
        }
    },
    "minimum-stability": "dev",
    "prefer-stable": true
}
